@model IEnumerable<HospitalTeam2.Models.Parking>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    Visitor Name
                </th>
                <th>
                    Car Number
                </th>
                <th>
                    Purpose of visit
                </th>
                <th>
                   Contact
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.VisitorName
                    </td>
                    <td>
                        @item.VisitoCarNo
                    </td>
                    <td>
                        @item.ParkingPurpose
                    </td>
                    <td>
                        @item.ParkingContact)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.ParkingID">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.ParkingID">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.ParkingID">Delete</a>
                    </td>
                </tr>
            }
           
    </tbody>
</table>
